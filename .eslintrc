{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "comma-dangle": [2],
    "no-dupe-args": [2],
    "no-dupe-keys": [2],
    "no-duplicate-case": [2],
    "no-extra-semi": [2],
    "no-irregular-whitespace": [2],
    "no-undef": [2],
    "valid-typeof": [2],
    "curly": [0],
    "dot-notation": [2],
    "eqeqeq": [2],
    "semi": [2, "always"],
    "quotes": [2, "single"],
    "no-unused-vars": [2],
    "array-bracket-spacing": [2, "never"],
    "brace-style": [2],
    "comma-spacing": [2],
    "comma-style": [2],
    "one-var": [2, "never"],
    "react/jsx-max-props-per-line": [2, {"maximum": 5}]
  }
}
