{
  "name": "react-karma-boilerplate",
  "version": "1.0.0",
  "description": "Simple boilerplate for React, Webpack and testing with Karma and Mocha",
  "main": "./src.entry.js",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --content-base public --port 3000",
    "test": "karma start",
    "watch:test": "karma start --no-single-run --browsers Firefox",
    "test:lint": "eslint \"src/**/*.js*\"",
    "pretest": "npm run test:lint"
  },
  "dependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.0.0"
  },
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-core": "^6.1.2",
    "babel-loader": "^6.1.0",
    "babel-polyfill": "^6.0.16",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "css-loader": "^0.18.0",
    "eslint": "^1.2.1",
    "eslint-plugin-react": "^3.2.3",
    "expect": "^1.13.0",
    "expect-jsx": "^2.1.2",
    "inject-loader": "^2.0.1",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "react-addons-test-utils": "^0.14.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.8",
    "webpack-dev-server": "^1.12.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
